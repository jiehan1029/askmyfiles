### This is the example .env file for local development

LOG_LEVEL="INFO"

# LLM
LLM_PROVIDER="google"  # select generator integration, possible values are "google" and "hf"
GOOGLE_API_KEY="your-api-token"
HF_API_TOKEN="your-api-token"

# haystack document store configs
DOCUMENT_STORE_NAME="qdrant"  # select document store, possible value is "in_memory" and "qdrant"
DOCUMENT_STORE_BACKUP_DIR="~/work/search-it/storage/backup"
QDRANT_DOCUMENT_STORE_STORAGE_DIR="../storage/qdrant"

# mangodb configs
MONGO_DB_NAME="chat_db"  # the db name to store chat history
MONGO_URI="mongodb://admin:admin123@localhost:27017"

# qdrant configs
QDRANT_URI="http://localhost:6333"
QDRANT_COLLECTION_NAME="documents"  # the collection name to store documents to search from
QDRANT_VECTOR_SIZE=384  # the vector size generated by the embedder
