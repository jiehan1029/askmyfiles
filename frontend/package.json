{
  "name": "search-it-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "*Search It!* electron app",
  "author": "Jie Han <jiehan1029@gmail.com>",
  "main": "public/electron.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "electron-is-dev": "^3.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env ELECTRON_IS_DEV=1 ELECTRON_START_URL=http://localhost:3000 concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build && cp public/electron.js build/",
    "electron-build": "electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "dev.jiehan1029.elecronapp",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "public/logo512.png",
      "target": "dmg"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "wait-on": "^8.0.3"
  }
}
